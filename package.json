        "@emotion/styled": "^11.11.5",
        "@smirea/node-utils": "github:smirea/smirea-node-utils",
        "@tanstack/react-query": "^5.51.21",
        "dotenv": "^16.4.5",
        "mobx": "^6.13.1",
        "mobx-react": "^9.1.1",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "ts-node": "^10.9.2",
        "ts-node-dev": "^2.0.0"
    },
    "devDependencies": {
        "@rsbuild/core": "1.0.0-alpha.5",
        "@rsbuild/plugin-react": "1.0.0-alpha.5",
        "@rsbuild/plugin-sass": "1.0.0-alpha.6",
        "@types/react": "^18.3.3",
        "@types/react-dom": "^18.3.0",
        "typescript": "^5.5.2"
