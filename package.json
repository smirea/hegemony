{
    "name": "@smirea/starter-template",
    "private": true,
    "version": "1.0.0",
    "scripts": {
        "server:dev": "ts-node-dev --respawn --transpile-only --ignore-watch node_modules server/src/index.ts",
        "client:dev": "rsbuild dev -c client/rsbuild.config.ts",
